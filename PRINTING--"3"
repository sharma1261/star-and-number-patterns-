def print_three(n=7):

    for row in range(n):
        for col in range(n):
          
            if row == 0 or row == n//2 or row == n-1:
                print("*", end="")
          
            elif col == n-1:
                print("*", end="")

            else:
                print(" ", end="")

        print()  

print_three()
